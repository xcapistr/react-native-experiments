{
  "expo": {
    "name": "react-native-experiments",
    "slug": "react-native-experiments",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xcapistr.reactnativeexperiments"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "24f8ca1b-fe96-4821-87ed-82e21d643fde"
      }
    },
    "resolver": {
      "sourceExts": ["js", "jsx", "json", "ts", "tsx", "cjs", "stl"],
      "assetExts": ["glb", "gltf", "mtl", "obj", "png", "jpg", "stl"]
    }
  }
}
